<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jxnu.dao.GoodMapper">

    <select id="queryBookByUserid" resultType="Goods">
        select * from goods
        where userid=#{arg0}
    </select>

    <select id="queryAllBook" resultType="Goods">
        select * from goods
    </select>

</mapper>